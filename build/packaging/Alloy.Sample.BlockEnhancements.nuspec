<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Alloy.Sample.BlockEnhancements</id>
    <version>$version$</version>
    <authors>EPiServer AB</authors>
    <owners>EPiServer AB</owners>
    <projectUrl>http://www.episerver.com/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Alloy Sample with BlockEnhancements</description>
    <releaseNotes />
  </metadata>
  <files>
    <file src="build\database\DefaultSiteContent.episerverdata" target="wwwroot\App_Data\DefaultSiteContent.episerverdata"/>
    <file src="build\database\*.cs" target="wwwroot\App_Code\" />
    <file src="build\packaging\transforms\*.transform" target="setup"/>
    <file src="build\packaging\*.sql" target="setup"/>
    <file src="packages\episerver.cms.core.*\tools\EPiServer.Cms.Core.sql" target="setup\sql"/>
    <file src="src\EPiServer.Labs.BlockEnhancements\bin\$configuration$\EPiServer.Labs.BlockEnhancements.*" target="wwwroot\bin" />
    <file src="src\AlloyMvcTemplates\bin\*.*" target="wwwroot\bin" exclude="src\AlloyMvcTemplates\bin\EPiServer.Labs.BlockEnhancements.*"/>
    <file src="src\AlloyMvcTemplates\ClientResources\**\*.*" target="wwwroot\ClientResources" />
    <file src="src\AlloyMvcTemplates\Resources\**\*.*" target="wwwroot\Resources" />
    <file src="src\AlloyMvcTemplates\Static\**\*.*" target="wwwroot\Static" />
    <file src="src\AlloyMvcTemplates\Views\**\*.*" target="wwwroot\Views" />
    <file src="src\AlloyMvcTemplates\modules\**\*.*" target="wwwroot\modules" />
    <file src="src\AlloyMvcTemplates\*.config" target="wwwroot" exclude="license.config"/>
    <file src="src\AlloyMvcTemplates\*.asax" target="wwwroot"/>
    <file src="src\AlloyMvcTemplates\*.ico" target="wwwroot"/>

    <file src="out\episerver-labs-block-enhancements.zip" target="wwwroot\modules\_protected\episerver-labs-block-enhancements" />
  </files>
</package>
